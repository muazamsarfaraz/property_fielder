<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Job Sequence -->
        <record id="seq_property_fielder_job" model="ir.sequence">
            <field name="name">Job Sequence</field>
            <field name="code">property_fielder.job</field>
            <field name="prefix">JOB</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>
        
        <!-- Route Sequence -->
        <record id="seq_property_fielder_route" model="ir.sequence">
            <field name="name">Route Sequence</field>
            <field name="code">property_fielder.route</field>
            <field name="prefix">ROUTE</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

        <!-- Change Request Sequence -->
        <record id="seq_property_fielder_change_request" model="ir.sequence">
            <field name="name">Change Request Sequence</field>
            <field name="code">property_fielder.change.request</field>
            <field name="prefix">CR</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
        </record>

    </data>
</odoo>

