<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Sequence for Inspection Templates -->
        <record id="seq_inspection_template" model="ir.sequence">
            <field name="name">Inspection Template</field>
            <field name="code">property_fielder.inspection.template</field>
            <field name="prefix">TPL-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequence for Inspections -->
        <record id="seq_inspection" model="ir.sequence">
            <field name="name">Inspection</field>
            <field name="code">property_fielder.inspection</field>
            <field name="prefix">INS-</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>

